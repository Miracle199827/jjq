<view class="findProject_content data-v-6ecf1bc8"><view class="screen data-v-6ecf1bc8"><picker class="screen_choose data-v-6ecf1bc8" mode="selector" value="{{typeIndex}}" range="{{typeArray}}" data-event-opts="{{[['change',[['typeChange',['$event']]]]]}}" onChange="__e"><block a:if="{{Choose1}}"><text class="data-v-6ecf1bc8">产权类型</text></block><block a:else><text class="data-v-6ecf1bc8">{{typeArray[typeIndex]}}</text></block><uni-icons vue-id="6b0ade14-1" type="arrowdown" color="999999" class="data-v-6ecf1bc8" onVueInit="__l"></uni-icons></picker><picker class="screen_choose data-v-6ecf1bc8" mode="selector" value="{{areaIndex}}" range="{{areaArray}}" data-event-opts="{{[['change',[['areaChange',['$event']]]]]}}" onChange="__e"><block a:if="{{Choose2}}"><text class="data-v-6ecf1bc8">面积</text></block><block a:else><text class="data-v-6ecf1bc8">{{areaArray[areaIndex]}}</text></block><uni-icons vue-id="6b0ade14-2" type="arrowdown" color="999999" class="data-v-6ecf1bc8" onVueInit="__l"></uni-icons></picker></view><view class="project_items data-v-6ecf1bc8" a:for="{{Project}}" a:for-item="project" a:for-index="__i0__"><view data-event-opts="{{[['tap',[['gotoDetails']]]]}}" onTap="__e" class="data-v-6ecf1bc8"><text class="items_title data-v-6ecf1bc8">{{project.name}}</text><text class="items_type data-v-6ecf1bc8">{{"产权类型："+project.type}}</text><text class="items_place data-v-6ecf1bc8">{{"交易地点："+project.place}}</text><text class="items_time data-v-6ecf1bc8">{{"公示日期："+project.time}}</text></view></view></view>